<h2>Search Author</h2>

<form action="/authors" method="get">
    <label for="name">Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <input type="submit" value="Search">
</form>

<br>
<% authors.forEach(element => { %>
    <div>
        <%=element.name %> 
    </div>
 <% }); %> 